<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.nihanapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.android.nihanapp.DiabetesActivity"
            android:parentActivityName="com.example.android.nihanapp.MainActivity" />
        <activity
            android:name="com.example.android.nihanapp.SafeSexActivity"
            android:parentActivityName="com.example.android.nihanapp.MainActivity" />
        <activity
            android:name="com.example.android.nihanapp.TravelActivity"
            android:parentActivityName="com.example.android.nihanapp.MainActivity" />
        <activity
            android:name="com.example.android.nihanapp.CaneWalkerActivity"
            android:parentActivityName="com.example.android.nihanapp.MainActivity" />
        <activity
            android:name="com.example.android.nihanapp.CareActivity"
            android:parentActivityName="com.example.android.nihanapp.MainActivity" />
        <activity
            android:name="com.example.android.nihanapp.WalkingActivity"
            android:parentActivityName="com.example.android.nihanapp.MainActivity" />
        <activity android:name="com.example.android.nihanapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.android.nihanapp.DetailActivity"
            android:parentActivityName="com.example.android.nihanapp.MainActivity"/>
    </application>

</manifest>